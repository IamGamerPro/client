declare($obj, $name)
	push($obj, $name)

extends($obj, $params = {})
	for $el in $obj
		{$el} ($params)

$iBase = ()
declare($iBase, i-base)

if file-exists("i-base_*.styl")
	@import "i-base_*.styl"

i-base($params = {})
	+cache($params)
		font-family "Helvetica Neue", Helvetica, Arial, sans-serif
