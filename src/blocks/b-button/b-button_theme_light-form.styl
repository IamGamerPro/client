declare(
	$bButton,
	b-button_theme_light-form
)

b-button_theme_light-form($p)
	$p = assign({
		theme: _theme_light-form,
		color: #000,
		gradColor1: #E6B200,
		gradColor2: #B27B00
	}, $p)

	b-button_theme_dark-form($p)
